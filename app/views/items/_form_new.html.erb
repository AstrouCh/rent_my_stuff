
<%= simple_form_for(@item) do |f| %>
  <%= f.input :title %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.association :category %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%# <%= f.input :average_rating %>
  <%= f.submit%>
<% end %>
