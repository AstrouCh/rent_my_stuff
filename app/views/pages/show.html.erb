<% if current_user == @user %>
<h1>These are your bookings</h1>

<% @bookings.each do |booking| %>

  <p><%= booking.item.title %></p>
  <p> Booking dates <%= booking.start_date %> to  <%= booking.end_date %> </p>

  <%= button_to "❌",
    booking_path(booking), method: :delete %>
  <% end %>

<% end %>

<p> These are <%= @user.first_name %>'s items</p>

<% @items.each do |item| %>

  <p> <%= link_to item.title, item_path %> </p>
<% end %>
